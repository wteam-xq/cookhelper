body {
	width: 1024px;
	height: 600px;
	padding: 0px;
	margin: 0px;
	overflow: hidden;
}

div canvas {
	padding: 0px;
	margin: 0px;
	overflow-y:scroll;
}

body #logo {
	width: 1024px;
	height: 600px;
	background-image: url(../images/wteam-logo.png);
	background-position: 0 0;
	background-repeat: no-repeat;
}

body div {
	width: 1024px;
	height: 600px;
	overflow-x:hidden;
}

#top {
	width: 1024px;
	height: 60px;
	float: left;
	background-color: #ede7e8;
	background-image: url(../images/top_bg.png);
	background-repeat: repeat-x;
	background-position: 0 100%;
	overflow-y:hidden;
}

#bottom {
	width: 1024px;
	height: 540px;
}

#bottomLeft {
	width: 60px;
	height: 100%;
	float: left;
	background-color: #d7b7b9;
	background-image: url(../images/left_bg.png);
	background-repeat: repeat-y;
	background-position: 100% 0;
	overflow-y:scroll;
}

#bottomMiddle {
	width: 204px;
	height: 100%;
	float: left;
	background-color: #e4c7c8;
	background-image: url(../images/middle_bg.png);
	background-repeat: repeat-y;
	background-position: 100% 0;
	overflow-y:scroll;
}

#bottomRight {
	width: 760px;
	height: 100%;
	float: left;
	background-color: #e8e8c5;
	overflow-y:scroll;
}

div#time{
	height:50px;
	width:180px;
	position:absolute;
	left:20px;
	top:4px;

}
input{
	width:50px;
	height:42px;
	font-size:20pt;
}

div#hour,#minute,#second{
	width:50px;
	height:50px;
	float:left;
	font-size:25pt;
}
div#all{
	position:absolute;
	top:9px;
	left:25px;
}

#left_prompt_box {
	background: url("../images/leftCanvasRect.png") no-repeat;
	color: #fff;
	font-size: 24px;
	font-weight: bold;
	position: absolute;
	top: 0px;
	left: 0px;
	display: none;
	text-align: center;
	vertical-align: middle;
	width: 114px;
	height: 50px;
	padding: 10px 0px 0px 5px;
}

#left_prompt_box span {
	
}
